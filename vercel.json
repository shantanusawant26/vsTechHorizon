{
    "version": 2,
    "name": "my-angular-node-app",
    "builds": [
      
      {
        "src": "frontend/package.json",
        "use": "@vercel/node"
      },
      {
        "src": "frontend",
        "use": "@vercel/static-build",
        "config": { "distDir": "frontend/docs" }
      },
      {
        "src": "backend/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "^/(.*)", "dest": "/backend/$1" },
      { "src": "^/(.*)", "dest": "/frontend/$1" }
    ]
  }
  