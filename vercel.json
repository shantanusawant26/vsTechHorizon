{
    "version": 2,
    "builds": [
      {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "frontend/docs" }
    },
 
      {
        "src": "backend/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/frontend/docs/index.html"}
    ]
  }
  